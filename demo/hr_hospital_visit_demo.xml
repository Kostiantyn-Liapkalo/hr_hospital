<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Visit Demo Data -->
        
        <record id="demo_visit_1" model="hr.hospital.visit">
            <field name="doctor_id" ref="demo_doctor_1"/>
            <field name="patient_id" ref="demo_patient_1"/>
            <field name="planned_datetime" eval="DateTime.now() - timedelta(days=5, hours=10)"/>
            <field name="actual_datetime" eval="DateTime.now() - timedelta(days=5, hours=11)"/>
            <field name="visit_type">first</field>
            <field name="state">completed</field>
            <field name="recommendations"><![CDATA[
                <p>Initial consultation recommendations:</p>
                <ul>
                    <li>Complete blood work panel</li>
                    <li>Chest X-ray</li>
                    <li>Follow-up in 2 weeks</li>
                    <li>Lifestyle modifications recommended</li>
                </ul>
            ]]></field>
            <field name="cost">150.00</field>
        </record>
        
        <record id="demo_visit_2" model="hr.hospital.visit">
            <field name="doctor_id" ref="demo_doctor_2"/>
            <field name="patient_id" ref="demo_patient_2"/>
            <field name="planned_datetime" eval="DateTime.now() - timedelta(days=10, hours=14)"/>
            <field name="actual_datetime" eval="DateTime.now() - timedelta(days=10, hours=15)"/>
            <field name="visit_type">follow_up</field>
            <field name="state">completed</field>
            <field name="recommendations"><![CDATA[
                <p>Follow-up consultation notes:</p>
                <ul>
                    <li>Blood pressure improved with medication</li>
                    <li>Continue current treatment plan</li>
                    <li>Monthly monitoring required</li>
                    <li>Dietary restrictions maintained</li>
                </ul>
            ]]></field>
            <field name="cost">100.00</field>
        </record>
        
        <record id="demo_visit_3" model="hr.hospital.visit">
            <field name="doctor_id" ref="demo_doctor_3"/>
            <field name="patient_id" ref="demo_patient_3"/>
            <field name="planned_datetime" eval="DateTime.now() - timedelta(days=2, hours=9)"/>
            <field name="actual_datetime" eval="DateTime.now() - timedelta(days=2, hours=9)"/>
            <field name="visit_type">emergency</field>
            <field name="state">completed</field>
            <field name="recommendations"><![CDATA[
                <p>Emergency treatment provided:</p>
                <ul>
                    <li>Immediate immobilization applied</li>
                    <li>Pain medication administered</li>
                    <li>X-ray confirmed fracture</li>
                    <li>Orthopedic consultation scheduled</li>
                </ul>
            ]]></field>
            <field name="cost">300.00</field>
        </record>
        
        <record id="demo_visit_4" model="hr.hospital.visit">
            <field name="doctor_id" ref="demo_doctor_4"/>
            <field name="patient_id" ref="demo_patient_4"/>
            <field name="planned_datetime" eval="DateTime.now() - timedelta(days=7, hours=11)"/>
            <field name="actual_datetime" eval="DateTime.now() - timedelta(days=7, hours=11)"/>
            <field name="visit_type">consultation</field>
            <field name="state">completed</field>
            <field name="recommendations"><![CDATA[
                <p>Allergy consultation results:</p>
                <ul>
                    <li>Allergy testing recommended</li>
                    <li>Antihistamines prescribed</li>
                    <li>Avoid known allergens</li>
                    <li>Follow-up with allergist</li>
                </ul>
            ]]></field>
            <field name="cost">120.00</field>
        </record>
        
        <record id="demo_visit_5" model="hr.hospital.visit">
            <field name="doctor_id" ref="demo_doctor_intern_1"/>
            <field name="patient_id" ref="demo_patient_5"/>
            <field name="planned_datetime" eval="DateTime.now() - timedelta(days=1, hours=15)"/>
            <field name="visit_type">first</field>
            <field name="state">in_progress</field>
            <field name="recommendations"><![CDATA[
                <p>Initial assessment by intern:</p>
                <ul>
                    <li>Patient symptoms noted</li>
                    <li>Preliminary diagnosis made</li>
                    <li>Awaiting mentor approval</li>
                    <li>Temporary treatment provided</li>
                </ul>
            ]]></field>
            <field name="cost">80.00</field>
        </record>
        
        <!-- Planned future visits -->
        <record id="demo_visit_6" model="hr.hospital.visit">
            <field name="doctor_id" ref="demo_doctor_1"/>
            <field name="patient_id" ref="demo_patient_1"/>
            <field name="planned_datetime" eval="DateTime.now() + timedelta(days=3, hours=10)"/>
            <field name="visit_type">follow_up</field>
            <field name="state">planned</field>
            <field name="cost">100.00</field>
        </record>
        
        <record id="demo_visit_7" model="hr.hospital.visit">
            <field name="doctor_id" ref="demo_doctor_2"/>
            <field name="patient_id" ref="demo_patient_6"/>
            <field name="planned_datetime" eval="DateTime.now() + timedelta(days=1, hours=14)"/>
            <field name="visit_type">preventive</field>
            <field name="state">planned</field>
            <field name="cost">90.00</field>
        </record>
        
        <!-- Cancelled visit -->
        <record id="demo_visit_8" model="hr.hospital.visit">
            <field name="doctor_id" ref="demo_doctor_3"/>
            <field name="patient_id" ref="demo_patient_7"/>
            <field name="planned_datetime" eval="DateTime.now() - timedelta(days=3, hours=16)"/>
            <field name="visit_type">follow_up</field>
            <field name="state">cancelled</field>
            <field name="recommendations"><![CDATA[
                <p>Visit cancelled by patient</p>
                <ul>
                    <li>Patient requested reschedule</li>
                    <li>New appointment pending</li>
                </ul>
            ]]></field>
            <field name="cost">0.00</field>
        </record>
        
        <!-- No show visit -->
        <record id="demo_visit_9" model="hr.hospital.visit">
            <field name="doctor_id" ref="demo_doctor_4"/>
            <field name="patient_id" ref="demo_patient_8"/>
            <field name="planned_datetime" eval="DateTime.now() - timedelta(days=4, hours=10)"/>
            <field name="visit_type">consultation</field>
            <field name="state">no_show</field>
            <field name="recommendations"><![CDATA[
                <p>Patient did not attend</p>
                <ul>
                    <li>No prior notification received</li>
                    <li>Rescheduling required</li>
                    <li>No-show fee applied</li>
                </ul>
            ]]></field>
            <field name="cost">25.00</field>
        </record>
        
        <!-- Multiple visits for same patient with different doctors -->
        <record id="demo_visit_10" model="hr.hospital.visit">
            <field name="doctor_id" ref="demo_doctor_5"/>
            <field name="patient_id" ref="demo_patient_9"/>
            <field name="planned_datetime" eval="DateTime.now() - timedelta(days=15, hours=13)"/>
            <field name="actual_datetime" eval="DateTime.now() - timedelta(days=15, hours=14)"/>
            <field name="visit_type">first</field>
            <field name="state">completed</field>
            <field name="recommendations"><![CDATA[
                <p>Pediatric consultation:</p>
                <ul>
                    <li>Child development normal</li>
                    <li>Vaccinations up to date</li>
                    <li>Regular check-ups recommended</li>
                </ul>
            ]]></field>
            <field name="cost">110.00</field>
        </record>
        
        <record id="demo_visit_11" model="hr.hospital.visit">
            <field name="doctor_id" ref="demo_doctor_5"/>
            <field name="patient_id" ref="demo_patient_10"/>
            <field name="planned_datetime" eval="DateTime.now() - timedelta(days=8, hours=10)"/>
            <field name="actual_datetime" eval="DateTime.now() - timedelta(days=8, hours=11)"/>
            <field name="visit_type">follow_up</field>
            <field name="state">completed</field>
            <field name="recommendations"><![CDATA[
                <p>Pediatric follow-up:</p>
                <ul>
                    <li>Growth parameters normal</li>
                    <li>Nutrition advice provided</li>
                    <li>Next visit in 3 months</li>
                </ul>
            ]]></field>
            <field name="cost">110.00</field>
        </record>
        
        <record id="demo_visit_12" model="hr.hospital.visit">
            <field name="doctor_id" ref="demo_doctor_6"/>
            <field name="patient_id" ref="demo_patient_11"/>
            <field name="planned_datetime" eval="DateTime.now() - timedelta(days=12, hours=8)"/>
            <field name="actual_datetime" eval="DateTime.now() - timedelta(days=12, hours=9)"/>
            <field name="visit_type">emergency</field>
            <field name="state">completed</field>
            <field name="recommendations"><![CDATA[
                <p>Surgical consultation:</p>
                <ul>
                    <li>Surgery scheduled for next week</li>
                    <li>Pre-operative tests required</li>
                    <li>Patient consent obtained</li>
                </ul>
            ]]></field>
            <field name="cost">200.00</field>
        </record>
    </data>
</odoo>